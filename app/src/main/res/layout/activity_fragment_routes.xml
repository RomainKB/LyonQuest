<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentRoutes">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".FragmentRoutes">

        <TextView
            android:id="@+id/activity_display_title"
            style="@style/TitleStyleGene"
            android:text="Liste des parcours"
            />
        <LinearLayout xmlns:android = "http://schemas.android.com/apk/res/android"
            android:orientation = "vertical"
            style="@style/ButtonStyleGene"
            android:id = "@+id/activity_display_routes_layoutOfDynamicContent">
        </LinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>